{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json",
        "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "prettier/prettier": ["error"],
      "@typescript-eslint/ban-types": "off",
      "no-unused-vars": "off"
    }
}
